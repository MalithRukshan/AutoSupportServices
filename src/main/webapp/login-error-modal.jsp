<!-- Login Error Modal -->
<div id="loginErrorModal" class="modal fade">
    <div class="modal-dialog modal-md" style="max-width: 400px">
        <div class="modal-content">
            <div class="modal-header" style="background-color: #ff6666">
                <button type="button" class="close" data-dismiss="modal"
                        aria-hidden="true">&times;</button>
                <h4 class="modal-title text-center">Ooops...Error occured..!</h4>
            </div>
            <div class="modal-body pull-center">
                <h2 id="common-error-h2">Your Username or Password is Incorrect</h2>
                <p class="text-warning">
                    <small>If you unable to login please contact our hot line or simmply drop a mail</small>
                </p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-dismiss="modal">Close</button>

            </div>
        </div>
    </div>
</div>
<!--./ Login Error Modal -->
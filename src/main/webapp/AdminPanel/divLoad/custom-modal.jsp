<!-- Delete Worning Modal HTML -->
<div id="customModal" class="modal fade">
    <div class="modal-dialog modal-md" style="max-width: 400px">
        <div class="modal-content">
            <div class="modal-header" style="background-color: #ff6666">
                <button type="button" class="close" data-dismiss="modal"
                        aria-hidden="true">&times;</button>
                <h4 class="modal-title text-center" id="custom-modal-title">Do you want to delete!</h4>
            </div>
            <div class="modal-body pull-center">
                <h2 id="custom-modal-body">Do you want to delete data!</h2>
                <p class="text-warning">
                    <small id="custom-modal-body-small">If you click ok , your all the data wiil be lost.</small>
                </p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" data-dismiss="modal" id="custom-modal-cancel-btn">Cancel</button>
                <a class="btn btn-danger" id="custom-modal-ok-btn">Ok</a>

            </div>
        </div>
    </div>
</div>
<!--/. Delete Error  Modal HTML -->